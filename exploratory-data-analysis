    "# plot cancellation reasons\n",
    "keys, counts = np.unique(data.CANCELLATION_REASON, return_counts=True)\n",
    "counts_norm = counts/sum(i > 0 for i in data.CANCELLATION_REASON)\n",
    "\n",
    "fig = plt.figure(figsize=(12, 6)) #specify figure size\n",
    "gs = gridspec.GridSpec(1, 2, width_ratios=[3,1]) #specify relative size of left and right plot\n",
    "\n",
    "#Absolute values\n",
    "ax0 = plt.subplot(gs[0])\n",
    "ax0 = plt.bar(['Airlines/Carriers'], counts[1], color=['pink'])\n",
    "ax0 = plt.bar(['Weather'], counts[2], color=['lightsalmon'])\n",
    "ax0 = plt.bar(['National Air System'], counts[3], color=['peru'])\n",
    "ax0 = plt.bar(['Security'], counts[4], color=['y'])\n",
    "ax0 = plt.title('Cancellation Reasons:\\n Absolute distribution') \n",
    "ax0 = plt.ylabel('Frequency')\n",
    "ax0 = plt.text(['Airlines/Carriers'], counts[1]/4, counts[1], fontsize=12, horizontalalignment='center', verticalalignment='center') \n",
    "ax0 = plt.text(['Weather'], counts[2]/4, counts[2], fontsize=12, horizontalalignment='center', verticalalignment='center') \n",
    "ax0 = plt.text(['National Air System'], counts[3]/4, counts[3], fontsize=12, horizontalalignment='center', verticalalignment='center') \n",
    "ax0 = plt.text(['Security'], counts[4]/4, counts[4], fontsize=12, horizontalalignment='center')\n",
    "\n",
    "#Normalized values\n",
    "ax1 = plt.subplot(gs[1])\n",
    "ax1 = plt.bar(['Cancelled Flights'], [counts_norm[1]], label='Airlines/Carriers', color=['pink'])\n",
    "ax1 = plt.bar(['Cancelled Flights'], [counts_norm[2]], bottom=counts_norm[1], label='Weather', color=['lightsalmon'])\n",
    "ax1 = plt.bar(['Cancelled Flights'], [counts_norm[3]], bottom=counts_norm[1]+counts_norm[2], label='National Air System', color=['peru'])\n",
    "ax1 = plt.bar(['Cancelled Flights'], [counts_norm[4]], bottom=counts_norm[1]+counts_norm[2]+counts_norm[3], label='Security', color=['y'])\n",
    "ax1 = plt.legend(bbox_to_anchor=(1, 1))\n",
    "ax1 = plt.title('Cancellation Reasons:\\n Relative distribution')\n",
    "ax1 = plt.ylabel('Frequency')\n",
    "ax1 = plt.text(['Cancelled Flights'],counts_norm[1]/4, '{}%'.format((counts_norm[1]*100).round(1)), fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "ax1 = plt.text(['Cancelled Flights'],(counts_norm[2]/4)+counts_norm[1]+counts_norm[3]+counts_norm[4], '{}%'.format((counts_norm[2]*100).round(1)), fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "ax1 = plt.text(['Cancelled Flights'],(counts_norm[3]/4)+counts_norm[1]+counts_norm[2]+counts_norm[4], '{}%'.format((counts_norm[3]*100).round(1)), fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "ax1 = plt.text(['Cancelled Flights'],(counts_norm[4]/4)+counts_norm[1]+counts_norm[2]+counts_norm[3], '{}%'.format((counts_norm[4]*100).round(1)), fontsize=12, horizontalalignment='center')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
