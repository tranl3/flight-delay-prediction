    "# plot delayed reasons\n",
    "sum1 = round(sum(data.AIR_SYSTEM_DELAY))\n",
    "sum2 = round(sum(data.SECURITY_DELAY))\n",
    "sum3 = round(sum(data.AIRLINE_DELAY))\n",
    "sum4 = round(sum(data.LATE_AIRCRAFT_DELAY))\n",
    "sum5 = round(sum(data.WEATHER_DELAY))\n",
    "\n",
    "sum1_norms = round(sum1/sum(data.ARRIVAL_DELAY), 2)\n",
    "sum2_norms = round(sum2/sum(data.ARRIVAL_DELAY), 2)\n",
    "sum3_norms = round(sum3/sum(data.ARRIVAL_DELAY), 2)\n",
    "sum4_norms = round(sum4/sum(data.ARRIVAL_DELAY), 2)\n",
    "sum5_norms = round(sum5/sum(data.ARRIVAL_DELAY), 2)\n",
    "\n",
    "fig = plt.figure(figsize=(14, 8)) #specify figure size\n",
    "gs = gridspec.GridSpec(1, 2, width_ratios=[3,1]) #specify relative size of left and right plot\n",
    "\n",
    "#Absolute values\n",
    "ax0 = plt.subplot(gs[0])\n",
    "ax0 = plt.bar(['Air System Delay'], sum1, color = ['pink'])\n",
    "ax0 = plt.bar(['Security Delay'], sum2, color = ['olivedrab'])\n",
    "ax0 = plt.bar(['Airline Delay'], sum3, color = ['lightsalmon'])\n",
    "ax0 = plt.bar(['Late Aircraft Delay'], sum4, color = ['peru'])\n",
    "ax0 = plt.bar(['Weather Delay'], sum5, color = ['y'])\n",
    "ax0 = plt.title('Arrival Delay Reasons (in Mins):\\n Absolute distribution') \n",
    "ax0 = plt.ylabel('Frequency')\n",
    "ax0 = plt.text(['Air System Delay'], sum1/5, sum1, fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "ax0 = plt.text(['Security Delay'], sum2/5, sum2, fontsize=12, horizontalalignment='center') \n",
    "ax0 = plt.text(['Airline Delay'], sum3/5, sum3, fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "ax0 = plt.text(['Late Aircraft Delay'], sum4/5, sum4, fontsize=12, horizontalalignment='center', verticalalignment='center') \n",
    "ax0 = plt.text(['Weather Delay'], sum5/5, sum5, fontsize=12, horizontalalignment='center')\n",
    "\n",
    "#Normalized values\n",
    "ax1 = plt.subplot(gs[1])\n",
    "ax1 = plt.bar(['Delayed Flights'], [sum1_norms], label='Air System Delay', color=['pink'])\n",
    "ax1 = plt.bar(['Delayed Flights'], [sum2_norms], bottom=sum1_norms, label='Security Delay', color=['olivedrab'])\n",
    "ax1 = plt.bar(['Delayed Flights'], [sum3_norms], bottom=sum1_norms+sum2_norms, label='Airline Delay', color=['lightsalmon'])\n",
    "ax1 = plt.bar(['Delayed Flights'], [sum4_norms], bottom=sum1_norms+sum2_norms+sum3_norms, label='Late Aircraft Delay', color=['peru'])\n",
