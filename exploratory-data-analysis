    "\n",
    "#Absolute distribution\n",
    "plt.subplot(1, 2, 1)\n",
    "ax1 = data.groupby(['DAY_OF_WEEK', 'ARRIVAL_DELAY_LEVEL'])['ARRIVAL_DELAY_LEVEL'].count().unstack().plot.bar(rot=0, ax=plt.gca(), width=0.7, color=colors)\n",
    "plt.title('Arrival Delay by Day of Week:\\n Absolute distribution') \n",
    "L = plt.legend()\n",
    "L.get_texts()[0].set_text('On Time (t < 15 min)')\n",
    "L.get_texts()[1].set_text('Arrival Delay (t >= 15 min)')\n",
    "#plot bar labels\n",
    "for p in ax1.patches:\n",
    "    ax1.annotate(str(p.get_height()), (p.get_x() * 1.005, p.get_height() * 0.605))\n",
    "\n",
    "\n",
    "#Relative distribution\n",
    "plt.subplot(1, 2, 2)\n",
    "ax2 = data.groupby(['DAY_OF_WEEK', 'ARRIVAL_DELAY_LEVEL'])['ARRIVAL_DELAY_LEVEL'].size().groupby(level=0).apply(\n",
    "    lambda x: 100 * x / x.sum()).unstack().plot(kind='bar',stacked=True, rot=0, ax=plt.gca(), color=colors)\n",
    "plt.gca().yaxis.set_major_formatter(mtick.PercentFormatter())\n",
    "plt.legend(bbox_to_anchor=(1, 1))\n",
    "plt.title('Arrival Delay by Day of Week:\\n Relative distribution') \n",
    "L = plt.legend()\n",
    "L.get_texts()[0].set_text('On Time (t < 15 min)')\n",
    "L.get_texts()[1].set_text('Arrival Delay (t >= 15 min)')\n",
    "#plot bar labels\n",
    "for p, q in zip(ax2.patches[0:7], ax2.patches[7:14]):\n",
    "    ax2.annotate(str(round(p.get_height(),1)) + '%', (p.get_x(), p.get_height()/3))\n",
    "    ax2.annotate(str(round(q.get_height(),1)) + '%', (q.get_x(), q.get_height()/3+p.get_height()))\n",
    "plt.show()\n",
    "plt.tight_layout()\n",
    "plt.show()"
