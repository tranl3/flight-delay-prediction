   "source": [
    "def departure_time_brackets(row):\n",
    "    \n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('00') or row['SCHEDULED_DEPARTURE'].startswith('01') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('02'):\n",
    "        return '00-03'\n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('03') or row['SCHEDULED_DEPARTURE'].startswith('04') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('05'):\n",
    "        return '03-06'\n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('06') or row['SCHEDULED_DEPARTURE'].startswith('07') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('08'):\n",
    "        return '06-09'\n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('09') or row['SCHEDULED_DEPARTURE'].startswith('10') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('11'):\n",
    "        return '09-12'\n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('12') or row['SCHEDULED_DEPARTURE'].startswith('13') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('14'):\n",
    "        return '12-15'\n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('15') or row['SCHEDULED_DEPARTURE'].startswith('16') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('17'):\n",
    "        return '15-18'\n",
    "    if row['SCHEDULED_DEPARTURE'].startswith('18') or row['SCHEDULED_DEPARTURE'].startswith('19') \\\n",
    "        or row['SCHEDULED_DEPARTURE'].startswith('20'):\n",
    "        return '18-21'\n",
    "    else:\n",
    "        return '21-24'\n",
    "        \n",
    "data = data.assign(SCHED_DEPARTURE=data.apply(departure_time_brackets, axis=1))\n",
    "data.head().style"
   ]
  },
