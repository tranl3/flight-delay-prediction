    "ax1 = plt.legend(bbox_to_anchor=(1, 1))\n",
    "ax1 = plt.title('Arrival Delay Reasons (in Mins):\\n Relative distribution')\n",
    "ax1 = plt.ylabel('Frequency')\n",
    "\n",
    "ax1 = plt.text(['Delayed Flights'],sum1_norms/5, '{}%'.format((sum1_norms*100)), fontsize=12, horizontalalignment='center')\n",
