    "\n",
    "#Normalized values\n",
    "ax1 = plt.subplot(gs[1])\n",
    "ax1 = plt.bar(['Arrival Delay'], [counts_norm[0]], label='On Time (t < 15 min', color=['pink'])\n",
    "ax1 = plt.bar(['Arrival Delay'], [counts_norm[1]], bottom=counts_norm[0], label='Arrival Delay (t >= 15 min)', color=['lightsalmon'])\n",
    "ax1 = plt.legend(bbox_to_anchor=(1, 1))\n",
    "ax1 = plt.title('Arrival Delay:\\n Relative distribution')\n",
    "ax1 = plt.ylabel('Frequency')\n",
    "ax1 = plt.text(['Arrival Delay'],counts_norm[0]/2, '{}%'.format((counts_norm[0]*100).round(1)),  fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "ax1 = plt.text(['Arrival Delay'],(counts_norm[1]/2)+counts_norm[0], '{}%'.format((counts_norm[1]*100).round(1)),  fontsize=12, horizontalalignment='center', verticalalignment='center')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
